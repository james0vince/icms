<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01//EN" "http://www.w3.org/TR/html4/strict.dtd">
<html>
<head>
<title><%= yield :title %></title>
<meta http-equiv="content-type" content="text/html; charset=iso-8859-1">
<%= stylesheet_link_tag "styles" %>
</head>
<body>
<div id="back-span">
	&nbsp;
</div>
<div id="container">
  <div id="header">

	<%= yield :header %>

  </div>
  <div id="wrapper">
    <div id="content">
        <div id="subNavigation">
		<%= yield :breadcrumbs %>      
	</div>
	<div id="main-body">
		<div id="main-content-main">
			<%= yield :main %>
		</div>
	</div>
	<div id="right-sidbar">
		 <%= yield :right %>
	</div>
    </div>
  </div>
  <div id="sidebar">
	 <%= yield :left %>
  </div>

  <div id="footer">

  </div>
</div>
<div id="back-span2">
	<%= yield :footer %>
</div>
<%= yield %>
</body>
</html>
